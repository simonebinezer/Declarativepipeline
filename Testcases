pipeline {
    agent any

    stages {
        stage('Checkout') {
            steps {
                script {
                    echo 'Checkout step'
                }
            }
        }

        stage('Build') {
            steps {
                script {
                    echo 'Build step'
                }
            }
        }

        stage('Test') {
            steps {
                script {
                    echo 'Test step'
                }
            }
        }

        stage('Deploy') {
            steps {
                script {
                    echo 'Deploy step'
                }
            }
        }
    }
}
